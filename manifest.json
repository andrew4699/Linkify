{
	"manifest_version": 2,

	"name": "Linkify",
	"description": "Color links based on load time.",
	"version": "1.0",

	"background":
	{
		"scripts": ["jquery-1.12.0.min.js", "main.js"]
	},

	"browser_action":
	{
		//"default_icon": "favicon.png",
		"default_popup": "main.html"
	},

	"permissions": ["webNavigation", "activeTab", "tabs", "background", "http://*/*", "https://*/*"]
}